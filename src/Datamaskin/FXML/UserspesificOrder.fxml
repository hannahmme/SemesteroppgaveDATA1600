<?xml version="1.0" encoding="UTF-8"?>

<!-- heia! Denne fxml-koden kan være ganske så identisk med den for alle ordre -->

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="822.0" prefWidth="751.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Datamaskin.FXML.UserspesificOrderController">
    <children>
        <Text layoutX="57.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hei superbruker. Her kan du se alle ordre som er gjennomført. Gjerne sorter etter dato om du vil" />
        <TableView fx:id="allOrders" layoutX="57.0" layoutY="189.0" prefHeight="590.0" prefWidth="636.0">
            <columns>
                <TableColumn fx:id="emailColumn" prefWidth="212.0" text="E-mail">
                    <cellValueFactory>
                        <PropertyValueFactory property="Email" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="orderIDColumn" prefWidth="145.0" text="OrderID">
                    <cellValueFactory>
                        <PropertyValueFactory property="OrderID" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="orderDateColumn" prefWidth="144.0" text="Date of order">
                    <cellValueFactory>
                        <PropertyValueFactory property="OrderDate" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="totalPriceColumn" prefWidth="134.0" text="Total price">
                    <cellValueFactory>
                        <PropertyValueFactory property="TotalPrice" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Button fx:id="toMainpage" layoutX="57.0" layoutY="23.0" mnemonicParsing="false" onAction="#toMainpage" onKeyPressed="#btnToMainpageEnter" text="Back to mainpage" />
        <TextField layoutX="506.0" layoutY="141.0" />
        <Text layoutX="428.0" layoutY="162.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Search" />
    </children>
</AnchorPane>
